<div class="example-container">
  <div class="top-bar">
    <div class="dropdown-container">
      <label class="question-label">Select user question</label>
      <select class="question-select" id="questionSelect">
        <option value="" disabled selected>Select a question</option>
        {questions.map((question) => (
          <option value={question}>{question}</option>
        ))}
      </select>
      <span class="dropdown-arrow">â–¼</span>
    </div>
    <div class="toggle-container">
      <label class="toggle-switch">
        <input type="checkbox" id="jailbreakPrefix">
        <span class="slider"></span>
        <span class="toggle-label">Add jailbreak prefix</span>
      </label>
      <label class="toggle-switch">
        <input type="checkbox" id="responseFormat">
        <span class="slider"></span>
        <span class="toggle-label">Add response format</span>
      </label>
      <label class="toggle-switch">
        <input type="checkbox" id="decoder">
        <span class="slider"></span>
        <span class="toggle-label">Add decoder</span>
      </label>
    </div>
  </div>

  <div class="response-container" id="responseContainer">
    <div class="response-row">
      <span class="label">User:</span>
      <div class="content user-content">
        <div id="jailbreakPrefixText" class="jailbreak-text"></div>
        <div id="userQuestion" class="user-text"></div>
        <div id="responseFormatText" class="response-format-text"></div>
      </div>
    </div>
    <div class="response-row llm-row">
      <span class="label">LLM:</span>
      <span class="content llm-text" id="llmResponse"></span>
    </div>
  </div>

  <div class="footer-note">
    *Jailbreak prefix generated using <a href="https://arxiv.org/pdf/2402.03299" target="_blank">GUARD</a>.
  </div>

