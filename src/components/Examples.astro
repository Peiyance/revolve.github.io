  }

  .toggle-switch {
    position: relative;
    display: inline-flex;
    align-items: center;
    color: #fff;
    font-size: 0.9rem;
  }

  .toggle-switch input {
    opacity: 0;
    width: 0;
    height: 0;
  }

  .slider {
    position: absolute;
    width: 40px;
    height: 20px;
    background-color: #ccc;
    border-radius: 10px;
    transition: background-color 0.3s;
    cursor: pointer;
  }

  .slider::before {
    content: "";
    position: absolute;
    width: 18px;
    height: 18px;
    background-color: #fff;
    border-radius: 50%;
    left: 2px;
    bottom: 1px;
    transition: transform 0.3s;
  }

  .toggle-switch input:checked + .slider {
    background-color: #4caf50;
  }

  .toggle-switch input:checked + .slider::before {
    transform: translateX(20px);
  }

  .toggle-label {
    margin-left: 45px;
  }

  .response-row {
    display: flex;
    align-items: flex-start;
    margin-bottom: 15px;
  }

  .llm-row {
    align-items: flex-start;
  }

  .response-row .label {
    font-weight: bold;
    margin-right: 10px;
    flex-shrink: 0;
    align-self: flex-start;
  }

  .content.user-content {
