  }

  .dropdown-container select option {
    color: #000;
  }

  .dropdown-arrow {
    font-size: 0.9rem;
    color: #fff;
    cursor: pointer;
    background-color: transparent;
    padding: 5px;
  }

  .toggle-container {
    display: flex;
